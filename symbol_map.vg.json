{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 800,
    "height": 450,
    "title": "Butterfly sightings in Australia between 2018 and 2023",
    "projection": {"type": "equalEarth"},
    "layer": [
      {
        "data": {
          "url": "C:\Users\janet\OneDrive\Desktop\HTML\js\map_aus.topojson",
          "format": {"type": "topojson", "feature": "ne_110m_graticules_30"}
        },
        "mark": {"type": "geoshape", "fill": "lightgray", "stroke": "white"}
      },

      {
        "data": {
          "url": "C:\Users\janet\OneDrive\Desktop\HTML\data\sighting_data.csv"
        },
        "mark": {"type": "circle", "tooltip": {"content": "data"}},
        "encoding": {
          "longitude": {"field": "decimalLongitude", "type": "quantitative"},
          "latitude": {"field": "decimalLatitude", "type": "quantitative"},
          "size": {
            "field": "individualCount",
            "type": "quantitative",
            "title": "number of sightings",
            "scale": {"domain": [1, 10000]}
          },
          
          }
        }
      }
    ]
  }